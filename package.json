    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "axios": "^1.7.4",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.12",
    "flag-icons": "^7.2.3",
